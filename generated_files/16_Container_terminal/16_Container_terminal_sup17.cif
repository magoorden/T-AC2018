    edge Unloadcrane_2_lift_Motor_c_rotateAbsoluteDegrees when not(Truck_Color_Sensor.detected_color = 0 and (Unloadcrane_2_Color_Sensor.detected_color = 2 and Unloadcrane_2_track_Motor.Idle));
