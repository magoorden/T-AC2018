    edge Unloadcrane_2_lift_Motor_c_rotateAbsoluteDegrees when not(Unloadcrane_2_Color_Sensor.detected_color = 0 and Unloadcrane_2_track_Motor.Idle);
    edge Unloadcrane_2_lift_Motor_u_stopped;
  location Place:
