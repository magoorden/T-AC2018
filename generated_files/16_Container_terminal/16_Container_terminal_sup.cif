    edge Start_Button_u_released when pushed do pushed := false;
end
plant automaton Loadplatform_switch_sensor:
  location Idle:
    initial;
    marked;
    edge Loadplatform_switch_sensor_c_notifyWhenClosed goto Reply;
  location Reply:
    edge Loadplatform_switch_sensor_u_closed goto Idle;
end
plant automaton Loadplatform_conveyor_motor:
  location Idle:
    initial;
    marked;
    edge Loadplatform_conveyor_motor_c_move_forward goto Moving;
  location Moving:
    edge Loadplatform_conveyor_motor_c_stop goto Stop;
  location Stop:
    edge Loadplatform_conveyor_motor_u_stopped goto Idle;
end
plant automaton Loadplatform_pre_conveyor_motor:
  location Idle:
    initial;
    marked;
    edge Loadplatform_pre_conveyor_motor_c_move_forward goto Moving;
  location Moving:
    edge Loadplatform_pre_conveyor_motor_c_stop goto Stop;
  location Stop:
    edge Loadplatform_pre_conveyor_motor_u_stopped goto Idle;
end
plant automaton Loadplatform_color_sensor:
  disc int[0..7] color_sensor_value = 6;
  location Idle:
    initial;
    marked;
    edge Loadplatform_color_sensor_c_get_color goto Wait_for_Reply;
  location Wait_for_Reply:
    edge Loadplatform_color_sensor_u_COLOR_RED do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_YELLOW do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_GREEN do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_BLUE do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_BLACK do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_WHITE do color_sensor_value := 7 goto Idle;
    edge Loadplatform_color_sensor_u_COLOR_UNDEFINED do color_sensor_value := 6 goto Idle;
end
plant automaton Loadplatform_Notify_Switch:
  location Initial:
    initial;
    edge Start_Button_u_pushed goto Running;
  location Running:
    marked;
    edge Loadplatform_switch_sensor_c_notifyWhenClosed;
    edge Start_Button_u_pushed;
end
plant automaton Restrict_behaviour_color_sensor_and_conveyors:
  location:
    initial;
    marked;
    edge Loadplatform_color_sensor_c_get_color when Loadplatform_switch_sensor.Reply;
    edge Loadplatform_pre_conveyor_motor_c_move_forward when Loadplatform_conveyor_motor.Moving and Loadplatform_color_sensor.color_sensor_value = 6;
    edge Loadplatform_conveyor_motor_c_move_forward when Loadplatform_switch_sensor.Reply;
    edge Loadplatform_conveyor_motor_u_stopped when Loadplatform_switch_sensor.Idle;
    edge Loadplatform_pre_conveyor_motor_u_stopped when Loadplatform_color_sensor.color_sensor_value = 7 or not Loadplatform_conveyor_motor.Moving;
end
plant automaton Loadcrane_Color_Sensor:
  disc int[0..7] detected_color = 6;
  location Receive_color:
    initial;
    marked;
    edge Loadcrane_Color_Sending_c_notify do detected_color := 6 goto Wait_for_Reply;
  location Wait_for_Reply:
    edge Loadcrane_Color_Sensor_u_detected_RED do detected_color := 0 goto Receive_color;
    edge Loadcrane_Color_Sensor_u_detected_BLUE do detected_color := 1 goto Receive_color;
    edge Loadcrane_Color_Sensor_u_detected_YELLOW do detected_color := 2 goto Receive_color;
    edge Loadcrane_Color_Sensor_u_detected_GREEN do detected_color := 3 goto Receive_color;
    edge Loadcrane_Color_Sensor_u_detected_BLACK do detected_color := 4 goto Receive_color;
    edge Loadcrane_Color_Sensor_u_detected_WHITE do detected_color := 5 goto Receive_color;
end
plant automaton Loadcrane_Color_Changing:
  disc int[0..7] req_color = 6;
  location Change_Color:
    initial;
    marked;
    edge Loadcrane_Color_Changing_c_change_color_to_load_pos_color when req_color != 0 do req_color := 0;
    edge Loadcrane_Color_Changing_c_change_color_to_unload_pos_1_color when req_color != 3 do req_color := 3;
    edge Loadcrane_Color_Changing_c_change_color_to_unload_pos_2_color when req_color != 2 do req_color := 2;
end
plant automaton Loadcrane_Color_Sending:
  location Send:
    initial;
    marked;
    edge Loadcrane_Color_Sending_c_notify;
end
plant automaton Loadcrane_track_Motor:
  disc int[0..2] motion = 2;
  location Idle:
    initial;
    marked;
    edge Loadcrane_track_Motor_c_move_forward do motion := 0 goto Moving;
    edge Loadcrane_track_Motor_c_move_backward do motion := 1 goto Moving;
  location Moving:
    edge Loadcrane_track_Motor_c_stop goto Stop;
  location Stop:
    edge Loadcrane_track_Motor_u_stopped goto Idle;
end
plant automaton Loadcrane_move_forward_and_backward:
  location:
    initial;
