plant automaton Control_Destination:
  disc int[0..3] loadcrane = 0;
  disc int[0..3] truck = 0;
  location Observe:
    initial;
    marked;
end
plant automaton Stop_Button:
  disc bool pushed = false;
  location:
    initial;
    marked;
    edge Stop_Button_u_pushed when not pushed do pushed := true;
    edge Stop_Button_u_released when pushed do pushed := false;
end
plant automaton Stop_System:
  location Running:
    initial;
    edge Stop_Button_u_pushed goto Stopped;
    edge Truck_Color_Changing_c_change_color_to_unload_pos_1_color;
    edge Truck_Color_Changing_c_change_color_to_unload_pos_2_color;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees;
    edge Unloadcrane_1_lift_Motor_c_rotateAbsoluteDegrees;
    edge Unloadcrane_2_lift_Motor_c_rotateAbsoluteDegrees;
  location Stopped:
    marked;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when Loadcrane_Activity_monitor.UP_Close;
    edge Unloadcrane_1_lift_Motor_c_rotateAbsoluteDegrees when Unloadcrane_1_Activity_monitor.UP_Close;
    edge Unloadcrane_2_lift_Motor_c_rotateAbsoluteDegrees when Unloadcrane_2_Activity_monitor.UP_Close;
    edge Stop_Button_u_pushed;
end
plant automaton Loadplatform_Notify_SwitchGlobal:
  location Initial:
    initial;
    edge Loadplatform_switch_sensor_c_notifyWhenClosed goto Running;
  location Running:
    marked;
    edge Loadplatform_switch_sensor_c_notifyWhenClosed when Loadcrane_Activity_monitor.UP_Close and Loadcrane_Color_Sensor.detected_color = 0;
end
plant automaton Truck_Change_requested_color:
  location Idle:
    initial;
    marked;
    edge Truck_Color_Changing_c_change_color_to_load_pos_color when Unloadcrane_1_Activity_monitor.UP_Close and Truck_Color_Sensor.detected_color = 3 or Unloadcrane_2_Activity_monitor.UP_Close and Truck_Color_Sensor.detected_color = 0 or Loadcrane_Activity_monitor.UP_Open and Control_Destination.truck = 3;
    edge Truck_Color_Changing_c_change_color_to_unload_pos_1_color when Loadcrane_Activity_monitor.UP_Open and Control_Destination.truck = 1 and Loadcrane_Color_Sensor.detected_color = 2;
    edge Truck_Color_Changing_c_change_color_to_unload_pos_2_color when Loadcrane_Activity_monitor.UP_Open and Control_Destination.truck = 2 and Loadcrane_Color_Sensor.detected_color = 2;
end
plant automaton Loadcrane_Change_requested_color:
  location Idle:
    initial;
    marked;
    edge Loadcrane_Color_Changing_c_change_color_to_load_pos_color when Loadcrane_Activity_monitor.UP_Open and (Control_Destination.loadcrane = 1 or Control_Destination.loadcrane = 2) and Truck_Color_Sensor.detected_color != 2 or Loadcrane_Activity_monitor.UP_Open and Control_Destination.loadcrane != 1 and Control_Destination.loadcrane != 2;
    edge Loadcrane_Color_Changing_c_change_color_to_unload_pos_1_color when Loadcrane_Activity_monitor.UP_Close and Control_Destination.loadcrane = 0 and Loadplatform_switch_sensor.Reply;
    edge Loadcrane_Color_Changing_c_change_color_to_unload_pos_2_color when Loadcrane_Activity_monitor.UP_Close and (Control_Destination.loadcrane = 1 or Control_Destination.loadcrane = 2) and Loadplatform_switch_sensor.Reply;
end
plant automaton Loadcrane_Activity_monitor:
  location UP_Open:
    initial;
    marked;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when Control_Destination.loadcrane = 0 and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 or (Control_Destination.loadcrane = 1 or Control_Destination.loadcrane = 2) and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 and Truck_Color_Sensor.detected_color = 2 and Loadcrane_track_Motor.Idle goto Pick;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when not(Control_Destination.loadcrane = 0 and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 or (Control_Destination.loadcrane = 1 or Control_Destination.loadcrane = 2) and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 and Truck_Color_Sensor.detected_color = 2 and Loadcrane_track_Motor.Idle);
    edge Loadcrane_lift_Motor_u_stopped;
  location Pick:
    marked;
    edge Loadcrane_lift_Motor_u_stopped goto UP_Close;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees;
  location UP_Close:
    marked;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when (Loadcrane_Color_Sensor.detected_color = 3 or Loadcrane_Color_Sensor.detected_color = 2) and Loadcrane_track_Motor.Idle goto Place;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when not((Loadcrane_Color_Sensor.detected_color = 3 or Loadcrane_Color_Sensor.detected_color = 2) and Loadcrane_track_Motor.Idle);
    edge Loadcrane_lift_Motor_u_stopped;
  location Place:
    marked;
    edge Loadcrane_lift_Motor_u_stopped goto UP_Open;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees;
end
plant automaton Loadcrane_Activity:
  location UP_Open:
    initial;
    marked;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when Control_Destination.loadcrane = 0 and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 or (Control_Destination.loadcrane = 1 or Control_Destination.loadcrane = 2) and Loadplatform_conveyor_motor.Idle and Loadcrane_Color_Sensor.detected_color = 0 and Truck_Color_Sensor.detected_color = 2 and Loadcrane_track_Motor.Idle goto Pick;
  location Pick:
    marked;
    edge Loadcrane_lift_Motor_u_stopped goto UP_Close;
  location UP_Close:
    marked;
    edge Loadcrane_lift_Motor_c_rotateAbsoluteDegrees when (Loadcrane_Color_Sensor.detected_color = 3 or Loadcrane_Color_Sensor.detected_color = 2) and Loadcrane_track_Motor.Idle goto Place;
  location Place:
    marked;
    edge Loadcrane_lift_Motor_u_stopped goto UP_Open;
end
plant automaton Unloadcrane_1_Activity_monitor:
  location UP_Open:
    initial;
    marked;
    edge Unloadcrane_1_lift_Motor_c_rotateAbsoluteDegrees when Truck_Color_Sensor.detected_color = 3 and Unloadcrane_1_Color_Sensor.detected_color = 2 and Unloadcrane_1_track_Motor.Idle goto Pick;
    edge Unloadcrane_1_lift_Motor_c_rotateAbsoluteDegrees when not(Truck_Color_Sensor.detected_color = 3 and (Unloadcrane_1_Color_Sensor.detected_color = 2 and Unloadcrane_1_track_Motor.Idle));
    edge Unloadcrane_1_lift_Motor_u_stopped;
  location Pick:
